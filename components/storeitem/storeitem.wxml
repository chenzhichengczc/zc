    <import src='../../components/star/star.wxml' />
    <template name='store'>
     <view class='store' bindtap='storelick'>
          <image src="{{store.photo}}" mode="aspectFill"></image>
          <view class='info'>
            <view class='name'>{{store.name}}</view>
            <view class='by'>by {{store.by}}</view>
            <!-- <view class='star_price'>
              <template is='star' data="{{count:store.star}}" /> {{store.price}}
            </view> -->
            <view class='type_km'>
              <view class='type'>已筹集 ￥{{store.price}}</view>
              <view class='km'>进度 ： {{store.rate}}%</view>
            </view>
          </view>
        </view>
        </template>